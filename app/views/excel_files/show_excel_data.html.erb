<h3>Courses uploaded from <%= @excel_file.file.filename %></h3>
<table class="table">
  <thead>
  <tr>
    <th>Course</th>
    <th>Syn</th>
    <th>Building</th>
    <th>Room</th>
    <th>Days</th>
    <th>Start Time</th>
    <th>End Time</th>
    <th>Action</th>
  </tr>
  </thead>
  <tbody>
  <% @courses.each do |course| %>
    <tr>
      <td><%= course.sec_name %></td>
      <td><%= course.syn %></td>
      <td><%= course.building %></td>
      <td><%= course.room %></td>
      <td><%= course.days %></td>
      <td><%= course.start_time %></td>
      <td><%= course.end_time %></td>
      <td>
        <%= link_to "Edit", course_path(course) %>
        <%= link_to "Delete", course_path(course), 'data-method' => 'delete', 'data-confirm' => 'Please confirm' %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>
<%= link_to "Back", excel_file_path, :class => "btn btn--primary"%>